{
  "expo": {
    "name": "Colorful Snake",
    "slug": "colorful-snake",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#a0d8f1"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.KimsGames.colorfulsnake",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "bundler": "metro"
    },
    "extra": {
      "eas": {
        "projectId": "375240b7-31bc-4441-913f-2687597053f6"
      }
    },
    "plugins": [
      "expo-audio",
      "expo-font",
      "./plugins/with-privacy-manifest.js",
      [
        "react-native-google-mobile-ads",
        {
          "iosAppId": "ca-app-pub-73492552188807012~2233111125"
        }
      ]
    ]
  },
  "react-native-google-mobile-ads": {
    "ios_app_id": "ca-app-pub-73492552188807012~2233111125"
  }
}
